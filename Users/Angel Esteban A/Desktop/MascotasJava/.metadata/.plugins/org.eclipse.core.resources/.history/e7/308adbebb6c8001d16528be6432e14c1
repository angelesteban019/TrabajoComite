
class Mascota{
private String nombre;
private String color;
private int edad;
private int id;
private String tipo;

public Mascota(String nombre, String color, int edad, int id, String tipo) {
    this.nombre = nombre;
    this.color = color;
    this.edad = edad;
    this.tipo = tipo;
}

public String getNombre() {
    return nombre;
}

public String getColor() {
    return color;
}

public int getEdad() {
    return edad;
}

public int getId() {
    return id;
}

public String getTipo() {
    return tipo;
}

@Override
public String toString() {
    return "Id: "+ id + "Nombre: " + nombre + ", Color: " + color + ", Edad: " + edad + ", Tipo: " + tipo;
}
}
